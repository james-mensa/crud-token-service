    "express": "^5.1.0",
    "express-async-handler": "^1.2.0",
    "http-errors": "^2.0.0",
    "http-status": "^2.1.0",
    "lodash": "^4.17.21",
    "mongoose": "^8.13.2",
    "status": "^0.0.13",
    "tsup": "^8.4.0",
