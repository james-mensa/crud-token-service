{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "WebWorker"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "paths": {
      "@hooks/*": ["./hooks/*"],
      "@components/*": ["./components/*"],
      "@assets/*": ["./assets/*"],
      "@apis/*": ["./apis/*"],
      "@config/*": ["./config/*"],
      "@routers/*": ["./routers/*"],
      "@styles/*": ["./styles/*"],
      "@utils/*": ["./utils/*"],
      "@pages/*": ["./pages/*"],
      "@store/*":["./store/*"]
    }

  },
  "include": [
    "src"
  ],

}
